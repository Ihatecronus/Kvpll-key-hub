# Railway Environment Variables
# Copy these to your Railway project settings

# Discord Bot Token
DISCORD_BOT_TOKEN=your_token_here

# Discord Webhook for logging
DISCORD_WEBHOOK=https://discordapp.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# Admin Channel ID
ADMIN_CHANNEL_ID=1470563050712072192

# Admin Token for API authentication
ADMIN_TOKEN=your_secure_admin_token

# Server Port (Railway will set this automatically, but you can override)
PORT=3000
